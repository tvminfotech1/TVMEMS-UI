{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "button-type": "off",
    "no-inline-styles": "off",
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off",
        "aria-required-children": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "input[type=month]"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not chrome <= 109",
    "not ios_saf <= 18.3",
    "not safari <= 18.3",
    "not samsung <= 27"
  ]
}